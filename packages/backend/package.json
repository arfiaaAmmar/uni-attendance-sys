{
  "name": "backend",
  "version": "1.0.0",
  "description": "Web server with Express, Typescript and MongoDB",
  "main": "src/server.js",
  "bunConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "ts-node -r dotenv/config -r tsconfig-paths/register ./src/server.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "start": "bun",
    "build": "tsc",
    "dev": "ts-node -r dotenv/config -r tsconfig-paths/register ./src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jwks-rsa": "^3.0.1",
    "mongodb": "^5.5.0",
    "mongoose": "^7.1.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.2",
    "nodemon": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}